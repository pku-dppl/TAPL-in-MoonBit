fn toplevel(Array[(Token, Pos, Pos)], initial_pos? : Pos) -> @list.T[Command]!ParseError

// Types and methods
type Command

type ErrorWithInfo

type Info

pub(all) struct LexEngine {
  graph : Array[(Int) -> (Int, Array[Array[Int]])]
  end_nodes : Array[(Int, Array[((Int, Int), (Int, Int))])?]
  start_tags : Array[Int]
  code_blocks_n : Int
}
impl LexEngine {
