}
impl Show for ParseError

type Pos

type Term

pub(all) enum Token {
  EOF
  ELSE
  FALSE
  IF
  IMPORT
  ISZERO
  PRED
  SUCC
  THEN
  TRUE
  INTV(Int)
  STRINGV(String)
  LPAREN
  RPAREN
  SEMI
}
impl Token {
  kind(Self) -> TokenKind
}
impl Show for Token

pub(all) enum TokenKind {
  TK_EOF
  TK_ELSE
  TK_FALSE
  TK_IF
  TK_IMPORT
  TK_ISZERO
  TK_PRED
  TK_SUCC
  TK_THEN
  TK_TRUE
  TK_INTV
  TK_STRINGV
  TK_LPAREN
  TK_RPAREN
  TK_SEMI
}
impl Show for TokenKind

